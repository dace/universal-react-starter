{
  "extends": "airbnb",
  "globals": {
    "__DEV__": false,
    "__PROD__": false,
    "__CLIENT__": false,
    "__SERVER__": false,
    "__DEBUG__": false
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "spaced-comment": 0,
    "new-cap": 0,
    "babel/generator-star-spacing": [2, "after"],
    "babel/new-cap": 2,
    "babel/object-curly-spacing": [0, "always"],
    "babel/object-shorthand": 2,
    "babel/arrow-parens": [2, "as-needed"],
    "max-len": [2, 80, 2]
  },
  "plugins": [
    "react",
    "babel"
  ]
}
